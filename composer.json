{
    "name": "tsfcorp/support",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "~3.4",
        "nesbot/carbon": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "TsfCorp\\Support\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TsfCorp\\Support\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "nesbot/carbon": "Required to use carbon_mysql_now()",
        "illuminate/database": "Required to use carbon_mysql_now()"
    }
}
